<div class="container">
  <div class="grid">
    {{#each notes}}
    <div class="grid-item">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">{{title}}</span>
          <p>{{body}}</p>
        </div>
        <div class="card-action">
          <a href="#">{{userid}}</a>
          <a href="#">{{pinned}}</a>
        </div>
      </div>
    </div>
    {{/each}}
  </div>
</div>
